{
    "$schema": "http://numengo.org/draft-05/schema",
    "$id": "http://numengo.org/draft-05/ngoschema/object-handlers",
    "definitions": {
        "ObjectHandler": {
            "type": "object",
            "abstract": true,
            "additionalProperties": false,
            "properties": {
                "objectClass": {
                    "type": "importable"
                },
                "keys": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "many": {
                    "type": "boolean",
                    "default": false
                }
            }
        },
        "FilterObjectHandlerMixin": {
            "type": "object",
            "abstract": true,
            "additionalProperties": false,
            "properties": {
                "only": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "but": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "recursive": {
                    "type": "boolean",
                    "default": false
                }
            }
        },
        "FileObjectHandler": {
            "type": "object",
            "abstract": true,
            "additionalProperties": false,
            "extends": [
                "#definitions/ObjectHandler",
                "#definitions/FilterObjectHandlerMixin"
            ],
            "properties": {
                "document": {
                    "$ref": "http://numengo.org/draft-05/ngoschema/document#/definitions/Document"
                },
                "overwrite": {
                    "type": "boolean",
                    "default": false
                }
            }
        },
        "JsonFileObjectHandler": {
            "type": "object",
            "extends": [
                "#definitions/FileObjectHandler"
            ],
            "additionalProperties": true
        },
        "YamlFileObjectHandler": {
            "type": "object",
            "extends": [
                "#definitions/FileObjectHandler"
            ],
            "additionalProperties": true
        },
        "Jinja2FileObjectHandler": {
            "type": "object",
            "extends": [
                "#definitions/FileObjectHandler"
            ],
            "additionalProperties": false,
            "properties": {
                "template": {
                    "type": "path"
                },
                "context": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "type": "object"
                },
                "protectedRegions": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "type": "object"
                }
            }
        },
        "Jinja2MacroFileObjectHandler": {
            "type": "object",
            "extends": [
                "#definitions/Jinja2FileObjectHandler"
            ],
            "additionalProperties": false,
            "properties": {
                "macroName": {
                    "type": "string"
                },
                "macroArgs": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "Jinja2MacroTemplatedPathFileObjectHandler": {
            "type": "object",
            "extends": [
                "#definitions/Jinja2MacroFileObjectHandler"
            ],
            "additionalProperties": false,
            "properties": {
                "outputDir": {
                    "type": "path"
                },
                "templatedPath": {
                    "type": "string"
                }
            }
        },
        "ObjectFactory": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "objectClass": {
                    "type": "importable"
                },
                "lazy_loading": {
                    "type": "boolean",
                    "default": true
                },
                "deserializers": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "ObjectLoader": {
            "type": "object",
            "extends": [
                "#/definitions/ObjectFactory"
            ],
            "additionalProperties": false,
            "properties": {
                "primaryKeys": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
