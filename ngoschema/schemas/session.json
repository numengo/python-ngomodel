{
    "$schema": "https://numengo.org/ngoschema/draft-06",
    "$id": "https://numengo.org/ngoschema/session",
    "type": "object",
    "$defs": {
        "Session": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "autoflush": {
                    "type": "boolean",
                    "default": true
                },
                "expire_on_commit": {
                    "type": "boolean",
                    "default": true
                },
                "autocommit": {
                    "type": "boolean",
                    "default": true
                },
                "bind": {
                    "type": "object",
                    "additionalProperties": true
                }
            }
        }
    }
}
